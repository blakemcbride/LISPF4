      IF(IARGS.EQ.0) GOTO 3105
      MAX = INDEX+IARGS-1
      DO 16060 I=INDEX,MAX
      IST=JILL(I)
      IF(IST .LE. NFREET) GOTO 25040
      S = GTREAL(IST,J)
      IF (S.NE.0..OR.IRFLAG.NE.NIL) GOTO 16044
      N = N+J
      GOTO 16060
16044 IF (IRFLAG.NE.NIL) GOTO 16046
      IRFLAG = T
      R = FLOAT(N)
16046 IF (S.EQ.0.) S = FLOAT(J)
      R = R+S
