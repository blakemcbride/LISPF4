      IF(IARGS.EQ.0) GOTO 3300
      N = JILL(INDEX) -NUMADD
      MAX=IARGS+INDEX-1
      IF (N.LT.1 .OR. N.GT.7) GOTO 25040
      IRES=DREG(N)
      IF(IARGS.EQ.1) GOTO 3300
      TEMP1 = JILL(INDEX+1)
      IF (TEMP1.GT.T) GOTO 25040
      DREG(N) = TEMP1
      GOTO 3300
