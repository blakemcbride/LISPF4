C     INTERLISP GIVES (CONS NIL ARG) FOR N<=0
12519 IRES=CONS(NIL,ARG)
      GOTO 998
C
C PACK
C
12520 ARG2=NCHARS(ARG,ARG2)-NUMADD
      ARG2 = PRTPOS-1
